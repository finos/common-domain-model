	<!-- End ClientAPI trade Types  -->
	<!-- DSMatch Trade Types  -->
	<xsd:element name="TRIChange">
		<xsd:complexType mixed="false">
			<xsd:sequence>
				<xsd:element ref="TradeReferenceInfo"/>
				<xsd:element ref="TRIChangeType"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="WorkflowUpdate">
		<xsd:complexType mixed="false">
			<xsd:sequence>
				<xsd:element ref="TradeReferenceInfo"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="EventProcessing">
		<xsd:complexType mixed="false">
			<xsd:sequence>
				<xsd:element ref="EventName" minOccurs="0"/>
				<xsd:element ref="EventId" minOccurs="0"/>
				<xsd:element ref="EventType"/>
				<xsd:element ref="EventProcessingType"/>
				<xsd:choice>
					<xsd:element ref="TradeReferenceInfo"/>
					<xsd:sequence>
						<xsd:element ref="fpml:FpML"/>
						<xsd:element ref="CreditSwapAdditionalFields" minOccurs="0" maxOccurs="1"/>
						<xsd:element ref="SwaptionAdditionalFields" minOccurs="0" maxOccurs="1"/>
					</xsd:sequence>
				</xsd:choice>
				<xsd:element ref="Amount" minOccurs="0"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="Payment">
		<xsd:complexType mixed="false">
			<xsd:sequence>
				<xsd:element ref="PaymentDetails" minOccurs="0"/>
				<xsd:sequence>
					<xsd:element ref="fpml:FpML"/>
					<xsd:element ref="CreditSwapAdditionalFields" minOccurs="0" maxOccurs="1"/>
					<xsd:element ref="SwaptionAdditionalFields" minOccurs="0" maxOccurs="1"/>
					<xsd:element ref="OriginalAdjustedPaymentDate" minOccurs="0" maxOccurs="1"/>
					<xsd:element ref="OriginalUnadjustedPaymentDate" minOccurs="0" maxOccurs="1"/>
				</xsd:sequence>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="Settlement">
		<xsd:complexType mixed="false">
			<xsd:sequence>
				<xsd:element ref="SettlementDetails" minOccurs="0"/>
				<xsd:sequence>
					<xsd:element ref="fpml:FpML"/>
					<xsd:element ref="CreditSwapAdditionalFields" minOccurs="0" maxOccurs="1"/>
					<xsd:element ref="SwaptionAdditionalFields" minOccurs="0" maxOccurs="1"/>
					<xsd:element ref="OriginalAdjustedPaymentDate" minOccurs="0"/>
					<xsd:element ref="OriginalUnadjustedPaymentDate" minOccurs="0"/>
				</xsd:sequence>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="FeeAmendment">
		<xsd:complexType mixed="false">
			<xsd:sequence>
				<xsd:choice>
					<xsd:sequence>
						<xsd:element ref="fpml:FpML"/>
						<xsd:element ref="CreditSwapAdditionalFields" minOccurs="0" maxOccurs="1"/>
						<xsd:element ref="SwaptionAdditionalFields" minOccurs="0" maxOccurs="1"/>
					</xsd:sequence>
					<xsd:element ref="TradeReferenceInfo"/>
				</xsd:choice>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!-- End DSMatch Trade Types  -->
	<!-- GTR Reporting Field Container starts here !-->
	<xsd:element name="ReportingUpdate">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="TradeReferenceInfo" minOccurs="0"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!-- New events for MCP - TIW Interface for Platform Unification  -->
	<!--<xsd:element name="Acknowledgement">
		<xsd:complexType mixed="false">
			<xsd:sequence>
				<xsd:element ref="TradeReferenceInfo"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>-->
	<xsd:element name="Exception">
		<xsd:complexType mixed="false">
			<xsd:sequence>
				<xsd:element ref="ExceptionTransType" minOccurs="0"/>
				<xsd:element ref="TradeReferenceInfo" minOccurs="0"/>
				<xsd:element ref="ExceptionReason" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!-- GTR Reporting Field Container ends here!-->
	<!-- End Level 2 ==============================================-->
	<!-- Begin Level 3+ ==============================================-->
	<xsd:element name="ExceptionTransType" type="rm:TransTypeDataType"/>
	<xsd:element name="ExceptionReason">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="ExceptionReasonCode"/>
				<xsd:element ref="ExceptionReasonDescription"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="TradeReferenceInfo">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:choice>
					<xsd:sequence>
						<xsd:element ref="Submitter"/>
						<xsd:element ref="TriggeringParty" minOccurs="0" maxOccurs="1"/>
						<xsd:element ref="OldTradeId" minOccurs="0" maxOccurs="1"/>
						<xsd:element ref="ContraTradeId" minOccurs="0" maxOccurs="1"/>
					</xsd:sequence>
					<xsd:element ref="BrokerTradeId"/>
					<xsd:choice minOccurs="1" maxOccurs="3">
						<xsd:element ref="YourTradeId"/>
						<xsd:element ref="OldTradeId" minOccurs="0" maxOccurs="1"/>
						<xsd:element ref="ContraTradeId" minOccurs="0" maxOccurs="1"/>
						<xsd:element ref="TriggeringParty" minOccurs="0" maxOccurs="1"/>
					</xsd:choice>
				</xsd:choice>
				<xsd:element ref="party" minOccurs="1" maxOccurs="3"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!-- The sellerPartyReference and buyerPartyReference elements are for supporting Terminate.
	Terminate is deprecated as of version 3.0 Replaced with Termination -->
	<xsd:element name="sellerPartyReference">
		<xsd:complexType>
			<xsd:attribute name="href" type="xsd:IDREF" use="required"/>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="buyerPartyReference">
		<xsd:complexType>
			<xsd:attribute name="href" type="xsd:IDREF" use="required"/>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="Assignment" substitutionGroup="PostTrade">
		<xsd:complexType mixed="false">
			<xsd:complexContent>
				<xsd:restriction base="PostTradeTransaction">
					<xsd:sequence>
						<xsd:choice>
							<xsd:sequence>
								<xsd:choice>
									<xsd:element ref="trade" minOccurs="1" maxOccurs="2"/>
									<xsd:sequence>
										<xsd:element ref="partyTradeIdentifier" minOccurs="1" maxOccurs="1"/>
										<xsd:element ref="notionalSchedule" minOccurs="0" maxOccurs="1"/>
										<xsd:element ref="trade" minOccurs="0" maxOccurs="1"/>
									</xsd:sequence>
								</xsd:choice>
								<xsd:element ref="TradeStatusReason" minOccurs="0"/>
								<xsd:element ref="IrsAdditionalFields" minOccurs="0" maxOccurs="1"/>
								<!-- Added as a part of Version 9 Credit changes -->
								<xsd:element ref="CreditSwapAdditionalFields" minOccurs="0" maxOccurs="1"/>
								<xsd:element ref="SwaptionAdditionalFields" minOccurs="0" maxOccurs="1"/>
								<xsd:element ref="AssignmentProvisions"/>
								<xsd:element ref="payment" minOccurs="0" maxOccurs="1"/>
								<!-- Changed for Portal API -->
								<xsd:element ref="party" minOccurs="1" maxOccurs="5"/>
							</xsd:sequence>
							<xsd:element ref="TradeReferenceInfo"/>
						</xsd:choice>
					</xsd:sequence>
				</xsd:restriction>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="AssignmentProvisions" substitutionGroup="provisions">
		<xsd:complexType mixed="false">
			<xsd:complexContent>
				<xsd:extension base="Provisions">
					<xsd:sequence>
						<xsd:element ref="Transferor"/>
						<xsd:element ref="Transferee"/>
						<xsd:element ref="RemainingParty"/>
						<xsd:element ref="RemainingPartyTwo" minOccurs="0" maxOccurs="1"/>
						<xsd:element ref="ConsentDateTime" minOccurs="0" maxOccurs="1"/>
						<xsd:element ref="NovationTradeDate" minOccurs="0" maxOccurs="1"/>
						<xsd:element ref="NovationDate"/>
						<xsd:element ref="NovatedAmount"/>
						<xsd:element ref="FullFirstCalculationPeriod" minOccurs="0" maxOccurs="1"/>
					</xsd:sequence>
				</xsd:extension>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="Termination" substitutionGroup="PostTrade">
		<xsd:complexType mixed="false">
			<xsd:complexContent>
				<xsd:restriction base="PostTradeTransaction">
					<xsd:sequence>
						<xsd:choice>
							<xsd:sequence>
								<xsd:choice>
									<xsd:element ref="trade" minOccurs="1" maxOccurs="1"/>
									<xsd:sequence>
										<xsd:element ref="partyTradeIdentifier" minOccurs="1" maxOccurs="1"/>
										<xsd:element ref="notionalSchedule" minOccurs="0" maxOccurs="1"/>
									</xsd:sequence>
								</xsd:choice>
								<xsd:element ref="TradeStatusReason" minOccurs="0"/>
								<xsd:element ref="IrsAdditionalFields" minOccurs="0" maxOccurs="1"/>
								<!-- Added as a part of Version 9 Credit changes -->
								<xsd:element ref="CreditSwapAdditionalFields" minOccurs="0" maxOccurs="1"/>
								<xsd:element ref="SwaptionAdditionalFields" minOccurs="0" maxOccurs="1"/>
								<xsd:element ref="TerminationProvisions"/>
								<xsd:element ref="payment" minOccurs="0" maxOccurs="1"/>
								<!-- Changed for Portal API -->
								<xsd:element ref="party" minOccurs="1" maxOccurs="3"/>
							</xsd:sequence>
							<xsd:element ref="TradeReferenceInfo"/>
						</xsd:choice>
					</xsd:sequence>
				</xsd:restriction>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="TerminationProvisions" substitutionGroup="provisions">
		<xsd:complexType mixed="false">
			<xsd:complexContent>
				<xsd:extension base="Provisions">
					<xsd:sequence>
						<xsd:element ref="TerminationTradeDate"/>
						<xsd:element ref="TerminationEffectiveDate"/>
						<xsd:choice>
							<xsd:element ref="Full"/>
							<xsd:element ref="Partial"/>
						</xsd:choice>
					</xsd:sequence>
				</xsd:extension>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	<!-- Added as a part of Version 9 Credit changes -->
	<xsd:element name="Exercise" substitutionGroup="PostTrade">
		<xsd:complexType mixed="false">
			<xsd:complexContent>
				<xsd:restriction base="PostTradeTransaction">
					<xsd:sequence>
						<xsd:choice>
							<xsd:sequence>
								<xsd:choice>
									<xsd:element ref="trade" minOccurs="1" maxOccurs="1"/>
									<xsd:sequence>
										<xsd:element ref="partyTradeIdentifier" minOccurs="1" maxOccurs="1"/>
										<xsd:element ref="notionalSchedule" minOccurs="0" maxOccurs="1"/>
									</xsd:sequence>
								</xsd:choice>
								<xsd:element ref="IrsAdditionalFields" minOccurs="0" maxOccurs="1"/>
								<!-- Added as a part of Version 9 Credit changes -->
								<xsd:element ref="CreditSwapAdditionalFields" minOccurs="0" maxOccurs="1"/>
								<xsd:element ref="SwaptionAdditionalFields" minOccurs="0" maxOccurs="1"/>
								<xsd:element ref="ExerciseProvisions"/>
								<xsd:element ref="payment" minOccurs="0" maxOccurs="1"/>
								<!-- Changed for Portal API -->
								<xsd:element ref="party" minOccurs="1" maxOccurs="3"/>
							</xsd:sequence>
							<xsd:element ref="TradeReferenceInfo"/>
						</xsd:choice>
					</xsd:sequence>
				</xsd:restriction>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="ExerciseProvisions" substitutionGroup="provisions">
		<xsd:complexType mixed="false">
			<xsd:complexContent>
				<xsd:extension base="Provisions">
					<xsd:sequence>
						<xsd:element ref="ExerciseTradeDate"/>
						<xsd:element ref="ExerciseEffectiveDate"/>
						<xsd:element ref="ExerciseEventType"/>
						<xsd:choice>
							<xsd:sequence>
								<xsd:element ref="ExercisedAmount"/>
								<xsd:element ref="OutstandingNotionalAmount" minOccurs="0" maxOccurs="1"/>
							</xsd:sequence>
							<xsd:sequence>
								<xsd:element ref="IncreaseInNumberOfOptions"/>
								<xsd:element ref="OutstandingNumberOfOptions"/>
							</xsd:sequence>
						</xsd:choice>
					</xsd:sequence>
				</xsd:extension>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="Increase" substitutionGroup="PostTrade">
		<xsd:complexType mixed="false">
			<xsd:complexContent>
				<xsd:restriction base="PostTradeTransaction">
					<xsd:sequence>
						<xsd:choice>
							<xsd:sequence>
								<xsd:choice>
									<xsd:element ref="trade" minOccurs="1" maxOccurs="1"/>
									<xsd:sequence>
										<xsd:element ref="partyTradeIdentifier" minOccurs="1" maxOccurs="1"/>
										<xsd:element ref="notionalSchedule" minOccurs="0" maxOccurs="1"/>
									</xsd:sequence>
								</xsd:choice>
								<xsd:element ref="IrsAdditionalFields" minOccurs="0" maxOccurs="1"/>
								<!-- Added as a part of Version 9 Credit changes -->
								<xsd:element ref="CreditSwapAdditionalFields" minOccurs="0" maxOccurs="1"/>
								<xsd:element ref="SwaptionAdditionalFields" minOccurs="0" maxOccurs="1"/>
								<xsd:element ref="IncreaseProvisions"/>
								<xsd:element ref="payment" minOccurs="0" maxOccurs="1"/>
								<xsd:element ref="party" minOccurs="1" maxOccurs="2"/>
							</xsd:sequence>
							<xsd:element ref="TradeReferenceInfo"/>
						</xsd:choice>
					</xsd:sequence>
				</xsd:restriction>
			</xsd:complexContent>
		</xsd:complexType>
