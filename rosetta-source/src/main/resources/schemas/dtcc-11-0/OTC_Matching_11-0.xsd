							</xsd:sequence>
							<xsd:sequence>
								<xsd:element ref="IncreaseInNumberOfOptions"/>
								<xsd:element ref="OutstandingNumberOfOptions"/>
							</xsd:sequence>
						</xsd:choice>
					</xsd:sequence>
				</xsd:extension>
			</xsd:complexContent>
		</xsd:complexType>
