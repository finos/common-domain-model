<?xml version="1.0" encoding="UTF-8"?>
<!-- TradeServ Schema Version 11.0 Rev 12 - January 3, 2018 -->
<!-- TradeServ External Version  -->
<!-- Upward compatable with DSMatch V11.0 Revision 11  -->
<!-- Updated with ESMA RTS 2.0 Changes - DSMatch V13.0  -->
<!-- Updated with MiFID Changes - DSMatch V13.1 -->
<!-- Header changes with Soap and absolute paths  -->
<!-- Response event type   -->
<!-- Repeating workflow fields, workflowPositionType  -->
<!-- ReservationTransType added to reserve notional transaction  -->
<!-- Client API elements commented out  -->
<!-- Static only fields commented out  -->
<xsd:schema xmlns="OTC_Matching_11-0" xmlns:rm="OTC_RM_11-0" xmlns:fpml="http://www.fpml.org/2010/FpML-4-9" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:ns1="http://schemas.xmlsoap.org/soap/envelope/" targetNamespace="OTC_Matching_11-0">
	<xsd:import namespace="http://www.fpml.org/2010/FpML-4-9" schemaLocation="../fpml-4-9/fpml-main-4-9.xsd"/>
	<xsd:import namespace="OTC_RM_11-0" schemaLocation="OTC_RM_11-0.xsd"/>
	<xsd:import namespace="http://schemas.xmlsoap.org/soap/envelope/" schemaLocation="soap-envelope.xsd"/>
	<!-- Begin Level 1 =============================================-->
	<xsd:element name="OTC_Matching">
		<xsd:complexType mixed="false">
			<xsd:sequence>
				<xsd:element ref="Activity"/>
				<xsd:element ref="Status"/>
				<xsd:element ref="ClearingDetails" minOccurs="0" maxOccurs="1"/>
				<xsd:element ref="WarehouseState" minOccurs="0" maxOccurs="2"/>
				<!-- Reporting Fields Starts here -->
				<xsd:element ref="ReportingState" minOccurs="0" maxOccurs="unbounded"/>
				<!-- Reporting Fields Ends here -->
				<xsd:choice>
					<xsd:element ref="Trade"/>
					<!-- Revision 9 - Remove BlockTrade transaction type and element  -->
					<!--					<xsd:element ref="BlockTrade"/>-->
					<!-- ClientAPI  -->
					<!--					<xsd:element ref="Block"/>-->
					<!-- End ClientAPI  -->
					<xsd:element ref="Backload"/>
					<xsd:element ref="PostTrade"/>
					<xsd:element ref="Exit"/>
					<xsd:element ref="Error"/>
					<xsd:element ref="Inquiry"/>
					<!-- ClientAPI  -->
					<!--					<xsd:element ref="Allocation"/>-->
					<!-- End ClientAPI  -->
					<xsd:element ref="TRIChange"/>
					<xsd:element ref="WorkflowUpdate"/>
					<xsd:element ref="Payment"/>
					<xsd:element ref="Settlement"/>
					<xsd:element ref="EventProcessing"/>
					<xsd:element ref="FeeAmendment"/>
					<xsd:element ref="ReserveNotional"/>
					<xsd:element ref="ReportingUpdate"/>
					<!-- ClientAPI  -->
					<!--					<xsd:element ref="AllegedDetails"/>-->
					<!-- End ClientAPI  -->
					<xsd:element ref="Exception"/>
				</xsd:choice>
				<xsd:element ref="DkDetail" minOccurs="0" maxOccurs="1"/>
				<xsd:element ref="WorkflowData" minOccurs="0" maxOccurs="3"/>
				<xsd:element ref="ReportingData" minOccurs="0" maxOccurs="1"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!-- End  Level 1 ===============================================-->
	<!-- Begin Level 2 ==============================================-->
	<xsd:element name="Trade">
		<xsd:complexType mixed="false">
			<xsd:sequence>
				<xsd:choice>
					<xsd:sequence>
						<xsd:element ref="fpml:FpML"/>
						<xsd:element ref="CreditSwapAdditionalFields" minOccurs="0" maxOccurs="1"/>
						<xsd:element ref="SwaptionAdditionalFields" minOccurs="0" maxOccurs="1"/>
						<xsd:element ref="fpml:bond" minOccurs="0" maxOccurs="1"/>
						<xsd:element ref="FloatingRateBondReference" minOccurs="0" maxOccurs="1"/>
						<xsd:element ref="MasterSupplementDate" minOccurs="0" maxOccurs="1"/>
						<xsd:element ref="PartyExecution" minOccurs="0" maxOccurs="1"/>
						<!-- ClientAPI TradeExecution  -->
						<!--						<xsd:element ref="TradeExecution" minOccurs="0" maxOccurs="1"/>-->
						<!-- ClientAPI TradeExecution  -->
						<xsd:element ref="PartyContact" minOccurs="0" maxOccurs="2"/>
						<xsd:element ref="TradeStatusReason" minOccurs="0"/>
					</xsd:sequence>
					<xsd:element ref="TradeReferenceInfo"/>
				</xsd:choice>
			</xsd:sequence>
			<xsd:attribute name="ShortDefaults" type="xsd:boolean"/>
		</xsd:complexType>
	</xsd:element>
	<!-- DSmatch Only -->
	<!-- Revision 9 - Remove BlockTrade transaction type and element  -->
