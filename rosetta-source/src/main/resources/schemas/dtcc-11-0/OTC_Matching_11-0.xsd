	<!-- DSmatch Only -->
	<xsd:element name="Backload">
		<xsd:complexType mixed="false">
			<xsd:sequence>
				<xsd:choice>
					<xsd:sequence>
						<xsd:element ref="fpml:FpML"/>
						<xsd:element ref="CreditSwapAdditionalFields" minOccurs="0" maxOccurs="1"/>
						<xsd:element ref="SwaptionAdditionalFields" minOccurs="0" maxOccurs="1"/>
						<xsd:element ref="BackloadEffectiveDate" minOccurs="1" maxOccurs="1"/>
					</xsd:sequence>
					<xsd:element ref="TradeReferenceInfo"/>
				</xsd:choice>
			</xsd:sequence>
			<xsd:attribute name="ShortDefaults" type="xsd:boolean"/>
		</xsd:complexType>
	</xsd:element>
	<!-- DSmatch Only -->
	<xsd:element name="Exit">
		<xsd:complexType mixed="false">
			<xsd:sequence>
				<xsd:element ref="TradeReferenceInfo"/>
				<xsd:element ref="ExitMsg" minOccurs="0"/>
				<xsd:element ref="AdditionalMsg" minOccurs="0"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="Error">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:sequence minOccurs="1" maxOccurs="unbounded">
					<xsd:element ref="ErrorMsgName"/>
					<xsd:element ref="ErrorString"/>
					<xsd:element ref="ErrorLocation"/>
					<xsd:element ref="ErrorCode"/>
				</xsd:sequence>
				<xsd:element ref="ErrorDetail"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="Position">
		<xsd:complexType mixed="false">
			<xsd:sequence>
				<xsd:choice>
					<xsd:sequence>
						<xsd:choice>
							<xsd:sequence>
								<xsd:element ref="fpml:FpML"/>
								<xsd:element ref="CreditSwapAdditionalFields" minOccurs="0" maxOccurs="1"/>
								<xsd:element ref="SwaptionAdditionalFields" minOccurs="0" maxOccurs="1"/>
								<xsd:element ref="BackloadEffectiveDate" minOccurs="0" maxOccurs="1"/>
								<xsd:element ref="OriginalFirstFixedRatePaymentDate" minOccurs="0" maxOccurs="1"/>
								<xsd:element ref="OriginalFirstPeriodAccrualStartDate" minOccurs="0" maxOccurs="1"/>
							</xsd:sequence>
							<xsd:element ref="NoTradeDetails" maxOccurs="1"/>
						</xsd:choice>
					</xsd:sequence>
					<xsd:element ref="TradeReferenceInfo"/>
				</xsd:choice>
				<xsd:element ref="party" minOccurs="0" maxOccurs="2"/>
			</xsd:sequence>
			<xsd:attribute name="ShortDefaults" type="xsd:boolean"/>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="Inquiry">
		<xsd:complexType>
			<xsd:choice>
				<xsd:sequence>
					<xsd:choice>
						<xsd:element ref="SearchResult"/>
						<xsd:sequence>
							<xsd:element ref="SearchResultEnd"/>
							<xsd:element ref="party" minOccurs="1" maxOccurs="1"/>
						</xsd:sequence>
						<xsd:element ref="party" minOccurs="1" maxOccurs="1"/>
					</xsd:choice>
				</xsd:sequence>
				<xsd:sequence>
					<xsd:element ref="Position"/>
					<xsd:element ref="CounterpartyReferenceInformation" minOccurs="0"/>
				</xsd:sequence>
			</xsd:choice>
		</xsd:complexType>
	</xsd:element>
	<!-- End DSmatch Only -->
