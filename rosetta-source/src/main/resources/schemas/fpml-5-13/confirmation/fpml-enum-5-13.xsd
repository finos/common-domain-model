    </xsd:annotation>
    <xsd:restriction base="xsd:token">
      <xsd:enumeration value="Ping" />
      <xsd:enumeration value="Plus1ToStop" />
      <xsd:enumeration value="Plus1ToPing" />
      <xsd:enumeration value="PushToPing" />
      <xsd:enumeration value="PushToStop" />
