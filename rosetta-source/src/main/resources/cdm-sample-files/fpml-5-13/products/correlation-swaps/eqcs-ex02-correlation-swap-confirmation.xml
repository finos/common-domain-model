                            <minimumBoundaryPercent>0.35123</minimumBoundaryPercent>
                            <!-- cap level -->
                            <maximumBoundaryPercent>0.85123</maximumBoundaryPercent>
                        </boundedCorrelation>
                        <numberOfDataSeries>1</numberOfDataSeries>
                    </correlation>
