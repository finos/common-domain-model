<!--
Example of CLP inflation swaps where both legs are fixed and inflation effect is captured on the UF leg of the swap. These are generally zero coupon swaps with maturity of 2Y or under.

CLP inflation swaps are represented as Xccy basis swaps. CLF is a non-circulating currency which essentially captures the inflation in form of an exchange rate. Based on need to trade CLP inflation swap as an offshore product, there arises a need to capture multiple FX rates â€“ CLF to CLP and CLP to USD; needed for the settlement of coupon.  Use of tags fxLinkedNotionalSchedule along with settlementRateOption provides the necessary framework to capture relative offset to absolute anchor date of the 2 FX rates required to be used for coupon settlement.
-->
<dataDocument xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www.fpml.org/FpML-5/confirmation" fpmlVersion="5-13" xsi:schemaLocation="http://www.fpml.org/FpML-5/confirmation /schemas/fpml-5-13/confirmation/fpml-main-5-13.xsd">
    <trade>
        <tradeHeader>
            <partyTradeIdentifier>
                <partyReference href="partyA"/>
                <tradeId tradeIdScheme="http://www.party-a.com/swaps/trade-id">596604984</tradeId>
            </partyTradeIdentifier>
            <partyTradeIdentifier>
                <partyReference href="partyB"/>
                <tradeId tradeIdScheme="http://www.party-b.com/swaps/trade-id">BCID71046476</tradeId>
            </partyTradeIdentifier>
            <partyTradeIdentifier>
                <partyReference href="partyC"/>
                <tradeId tradeIdScheme="http://www.swapswire.com/spec/2001/trade-id-1-0">184467696-2</tradeId>
            </partyTradeIdentifier>
            <tradeDate>2022-05-03</tradeDate>
        </tradeHeader>
        <swap>
            <productType>InterestRateSwap</productType>
            <swapStream id="fixedLeg">
                <payerPartyReference href="partyA"/>
                <receiverPartyReference href="partyB"/>
                <calculationPeriodDates id="fixedLegCalcPeriodDates">
                    <effectiveDate>
                        <unadjustedDate>2022-09-21</unadjustedDate>
                        <dateAdjustments>
                            <businessDayConvention>NONE</businessDayConvention>
                        </dateAdjustments>
                    </effectiveDate>
                    <terminationDate>
                        <unadjustedDate>2024-09-21</unadjustedDate>
                        <dateAdjustments>
                            <businessDayConvention>MODFOLLOWING</businessDayConvention>
                            <businessCenters>
                                <businessCenter>USNY</businessCenter>
                                <businessCenter>CLSA</businessCenter>
                            </businessCenters>
                        </dateAdjustments>
                    </terminationDate>
                    <calculationPeriodDatesAdjustments>
                        <businessDayConvention>MODFOLLOWING</businessDayConvention>
                        <businessCenters>
                            <businessCenter>USNY</businessCenter>
                            <businessCenter>CLSA</businessCenter>
                        </businessCenters>
                    </calculationPeriodDatesAdjustments>
                    <calculationPeriodFrequency>
                        <periodMultiplier>1</periodMultiplier>
                        <period>T</period>
                        <rollConvention>NONE</rollConvention>
                    </calculationPeriodFrequency>
                </calculationPeriodDates>
                <paymentDates id="fixedLegPaymentDates">
                    <calculationPeriodDatesReference href="fixedLegCalcPeriodDates"/>
                    <paymentFrequency>
                        <periodMultiplier>1</periodMultiplier>
                        <period>T</period>
                    </paymentFrequency>
                    <payRelativeTo>CalculationPeriodEndDate</payRelativeTo>
                    <paymentDatesAdjustments>
                        <businessDayConvention>MODFOLLOWING</businessDayConvention>
                        <businessCenters>
                            <businessCenter>USNY</businessCenter>
                            <businessCenter>CLSA</businessCenter>
                        </businessCenters>
                    </paymentDatesAdjustments>
                </paymentDates>
                <calculationPeriodAmount>
                    <calculation>
                        <notionalSchedule id="notionalScheduleCLP">
                            <notionalStepSchedule>
                                <initialValue>16713460000</initialValue>
                                <currency>CLP</currency>
                            </notionalStepSchedule>
                        </notionalSchedule>
                        <fixedRateSchedule>
                            <initialValue>0.0</initialValue>
                        </fixedRateSchedule>
                        <dayCountFraction>ACT/360</dayCountFraction>
                    </calculation>
                </calculationPeriodAmount>
                <principalExchanges>
                    <initialExchange>true</initialExchange>
                    <finalExchange>true</finalExchange>
                    <intermediateExchange>true</intermediateExchange>
                </principalExchanges>
                <settlementProvision>
                    <settlementCurrency>USD</settlementCurrency>
                    <nonDeliverableSettlement>
                        <referenceCurrency>CLP</referenceCurrency>
                        <fxFixingDate>
                            <periodMultiplier>-2</periodMultiplier>
                            <period>D</period>
                            <dayType>Business</dayType>
                            <businessDayConvention>NONE</businessDayConvention>
                            <businessCenters>
                                <businessCenter>USNY</businessCenter>
                                <businessCenter>CLSA</businessCenter>
                            </businessCenters>
                            <dateRelativeToPaymentDates>
                                <paymentDatesReference href="fixedLegPaymentDates"/>
                            </dateRelativeToPaymentDates>
                        </fxFixingDate>
                        <settlementRateOption>CLP.DOLAR.OBS/CLP10</settlementRateOption>
                    </nonDeliverableSettlement>
                </settlementProvision>
            </swapStream>
            <swapStream id="fixedLeg2">
                <payerPartyReference href="partyB"/>
                <receiverPartyReference href="partyA"/>
                <calculationPeriodDates id="fixedLeg2CalcPeriodDates">
                    <effectiveDate>
                        <unadjustedDate>2022-09-21</unadjustedDate>
                        <dateAdjustments>
                            <businessDayConvention>NONE</businessDayConvention>
                        </dateAdjustments>
                    </effectiveDate>
                    <terminationDate>
                        <unadjustedDate>2032-09-21</unadjustedDate>
                        <dateAdjustments>
                            <businessDayConvention>MODFOLLOWING</businessDayConvention>
                            <businessCenters>
                                <businessCenter>USNY</businessCenter>
                                <businessCenter>CLSA</businessCenter>
                            </businessCenters>
                        </dateAdjustments>
                    </terminationDate>
                    <calculationPeriodDatesAdjustments>
                        <businessDayConvention>MODFOLLOWING</businessDayConvention>
                        <businessCenters>
                            <businessCenter>USNY</businessCenter>
                            <businessCenter>CLSA</businessCenter>
                        </businessCenters>
                    </calculationPeriodDatesAdjustments>
                    <calculationPeriodFrequency>
                        <periodMultiplier>1</periodMultiplier>
                        <period>T</period>
                        <rollConvention>NONE</rollConvention>
                    </calculationPeriodFrequency>
                </calculationPeriodDates>
                <paymentDates id="fixedLeg2PaymentDates">
                    <calculationPeriodDatesReference href="fixedLeg2CalcPeriodDates"/>
                    <paymentFrequency>
                        <periodMultiplier>1</periodMultiplier>
                        <period>T</period>
                    </paymentFrequency>
                    <payRelativeTo>CalculationPeriodEndDate</payRelativeTo>
                    <paymentDatesAdjustments>
                        <businessDayConvention>MODFOLLOWING</businessDayConvention>
                        <businessCenters>
                            <businessCenter>USNY</businessCenter>
                            <businessCenter>CLSA</businessCenter>
                        </businessCenters>
                    </paymentDatesAdjustments>
                </paymentDates>
                <calculationPeriodAmount>
                    <calculation>
                        <fxLinkedNotionalSchedule>
                            <constantNotionalScheduleReference href="notionalScheduleCLP"/>
                            <initialValue>500000</initialValue>
                            <varyingNotionalCurrency>CLF</varyingNotionalCurrency>
                            <varyingNotionalFixingDates>
                                <periodMultiplier>0</periodMultiplier>
                                <period>D</period>
                                <businessDayConvention>NONE</businessDayConvention>
                                <businessCenters>
                                    <businessCenter>CLSA</businessCenter>
                                </businessCenters>
                                <dateRelativeTo href="fixedLeg2PaymentDates"/>
                            </varyingNotionalFixingDates>
                            <fxSpotRateSource>
                                <primaryRateSource>
                                    <rateSource>BancoCentralChile</rateSource>
                                </primaryRateSource>
                                <fixingTime>
                                    <hourMinuteTime>17:00:00</hourMinuteTime>
                                    <businessCenter>CLSA</businessCenter>
                                </fixingTime>
                            </fxSpotRateSource>
                            <varyingNotionalInterimExchangePaymentDates>
                                <periodMultiplier>0</periodMultiplier>
                                <period>D</period>
                                <businessDayConvention>NONE</businessDayConvention>
                                <dateRelativeTo href="fixedLeg2PaymentDates"/>
                            </varyingNotionalInterimExchangePaymentDates>
                        </fxLinkedNotionalSchedule>
                        <fixedRateSchedule>
                            <initialValue>0.0</initialValue>
                        </fixedRateSchedule>
                        <dayCountFraction>ACT/360</dayCountFraction>
                    </calculation>
                </calculationPeriodAmount>
                <principalExchanges>
                    <initialExchange>true</initialExchange>
                    <finalExchange>true</finalExchange>
                    <intermediateExchange>true</intermediateExchange>
                </principalExchanges>
                <settlementProvision>
                    <settlementCurrency>USD</settlementCurrency>
                    <nonDeliverableSettlement>
                        <referenceCurrency>CLP</referenceCurrency>
                        <fxFixingDate>
                            <periodMultiplier>-2</periodMultiplier>
                            <period>D</period>
                            <dayType>Business</dayType>
                            <businessDayConvention>NONE</businessDayConvention>
                            <businessCenters>
                                <businessCenter>USNY</businessCenter>
                                <businessCenter>CLSA</businessCenter>
                            </businessCenters>
                            <dateRelativeToPaymentDates>
                                <paymentDatesReference href="fixedLeg2PaymentDates"/>
                            </dateRelativeToPaymentDates>
                        </fxFixingDate>
                        <settlementRateOption>CLP.DOLAR.OBS/CLP10</settlementRateOption>
                    </nonDeliverableSettlement>
                </settlementProvision>
            </swapStream>
        </swap>
    </trade>
    <party id="partyA">
        <partyId partyIdScheme="http://www.fpml.org/coding-scheme/external/iso17442">549300ABANKV6BYQOWM67</partyId>
        <partyName>AAA International</partyName>
    </party>
    <party id="partyB">
        <partyId partyIdScheme="http://www.fpml.org/coding-scheme/external/iso17442">529900CPTY57S5UCBB52</partyId>
        <partyName>AAA Capital Services Inc.</partyName>
    </party>
    <party id="partyC">
        <partyId partyIdScheme="http://www.fpml.org/coding-scheme/external/iso17442">529900AYEI57S5UCED35</partyId>
        <partyName>SWAPSWIRE</partyName>
    </party>
</dataDocument>

