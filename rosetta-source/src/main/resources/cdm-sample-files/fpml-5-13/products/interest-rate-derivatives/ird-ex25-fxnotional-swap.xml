                            <businessCenter>USNY</businessCenter>
                            <businessCenter>JPTO</businessCenter>
                        </businessCenters>
                    </paymentDatesAdjustments>
                </paymentDates>
                <calculationPeriodAmount>
                    <calculation>
                        <notionalSchedule id="notionalScheduleJPY">
