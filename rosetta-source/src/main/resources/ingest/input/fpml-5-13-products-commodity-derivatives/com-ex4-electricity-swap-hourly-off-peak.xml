                        <businessDayConvention>NONE</businessDayConvention>
                    </paymentDaysOffset>
                    <businessCenters>
                        <businessCenter>USNY</businessCenter>
                    </businessCenters>
                </relativePaymentDates>
            </floatingLeg>
