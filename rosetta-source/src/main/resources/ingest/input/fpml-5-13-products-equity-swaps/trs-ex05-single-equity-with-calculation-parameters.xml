<!-- 
Example of a total return swap, demonstrating the specification of calculation parameters for accrual conventions, business center and lookback period, on the interest leg of the swap.
-->
<!--5.0:Message type is a Root of the message-->
<dataDocument xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www.fpml.org/FpML-5/confirmation" fpmlVersion="5-13" xsi:schemaLocation="http://www.fpml.org/FpML-5/confirmation /schemas/fpml-5-13/confirmation/fpml-main-5-13.xsd">
    <trade id="trs-eq1-trade">
        <tradeHeader>
            <partyTradeIdentifier>
                <partyReference href="Party_1"/>
                <tradeId tradeIdScheme="http://www.abc.com/swaps/trade-id">TRS-01</tradeId>
            </partyTradeIdentifier>
            <partyTradeIdentifier>
                <partyReference href="Party_2"/>
                <tradeId tradeIdScheme="http://www.hedgeco.com/swaps/trade-id">total-ret-swap-01</tradeId>
            </partyTradeIdentifier>
            <tradeDate>2021-08-18</tradeDate>
        </tradeHeader>
        <returnSwap>
            <productType productTypeScheme="http://www.fpml.org/coding-scheme/product-type-simple">TotalReturnSwap</productType>
            <buyerPartyReference href="Party_1"/>
            <sellerPartyReference href="Party_2"/>
            <returnLeg id="id_ReturnLeg">
                <legIdentifier>
                    <legId legIdScheme="http://www.partyA.com/leg-id">12345</legId>
                </legIdentifier>
                <payerPartyReference href="Party_2"/>
                <receiverPartyReference href="Party_1"/>
                <effectiveDate>
                    <adjustableDate>
                        <unadjustedDate>2021-08-20</unadjustedDate>
                        <dateAdjustments>
                            <businessDayConvention>NONE</businessDayConvention>
                        </dateAdjustments>
                    </adjustableDate>
                </effectiveDate>
                <terminationDate>
                    <adjustableDate>
                        <unadjustedDate>2022-08-31</unadjustedDate>
                        <dateAdjustments>
                            <businessDayConvention>NONE</businessDayConvention>
                        </dateAdjustments>
                    </adjustableDate>
                </terminationDate>
                <underlyer>
                    <singleUnderlyer>
                        <equity>
                            <instrumentId instrumentIdScheme="http://www.fpml.org/spec/2002/instrument-id-Bloomberg-1-0">X US</instrumentId>
                            <description>US STEEL CORP</description>
                            <currency>USD</currency>
                        </equity>
                        <openUnits>212000.0000</openUnits>
                        <dividendPayout>
