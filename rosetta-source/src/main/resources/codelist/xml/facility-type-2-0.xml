   <SimpleCodeList>
      <Row>
         <Value>
            <SimpleValue>BridgeLoan</SimpleValue>
         </Value>
         <Value>
            <SimpleValue>FpML</SimpleValue>
         </Value>
         <Value>
            <SimpleValue>Bridge loan.</SimpleValue>
         </Value>
      </Row>
      <Row>
         <Value>
            <SimpleValue>DelayedDrawTerm</SimpleValue>
         </Value>
         <Value>
            <SimpleValue>FpML</SimpleValue>
         </Value>
         <Value>
            <SimpleValue>Delayed Draw Term.</SimpleValue>
         </Value>
      </Row>
      <Row>
         <Value>
            <SimpleValue>LetterOfCredit</SimpleValue>
         </Value>
         <Value>
            <SimpleValue>FpML</SimpleValue>
         </Value>
         <Value>
            <SimpleValue>Letter of credit, i.e. commitment by a bank or syndicate to provide a
