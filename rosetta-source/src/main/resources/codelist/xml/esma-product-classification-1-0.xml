      <CanonicalVersionUri>http://www.fpml.org/coding-scheme/esma-product-classification-1-0</CanonicalVersionUri>
      <LocationUri>http://www.fpml.org/coding-scheme/esma-product-classification-1-0.xml</LocationUri>
   </Identification>
   <ColumnSet>
