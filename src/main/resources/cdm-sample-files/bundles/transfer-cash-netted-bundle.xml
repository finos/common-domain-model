        <co:timestamp>
            <co:creationTimestamp>2018-08-22T08:08:48.0</co:creationTimestamp>
        </co:timestamp>
        <co:eventIdentifier>
            <co:issuer href="party10"/>
            <co:identifier>obs-2</co:identifier>
            <co:version>1</co:version>
        </co:eventIdentifier>
