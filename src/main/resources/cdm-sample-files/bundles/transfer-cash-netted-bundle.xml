    <co:event>
        <!-- Payment lifecycle event that takes place at the termination of the zero coupon swap
            and is made of 2 transfer primitives, 1 per currency -->
        <co:timestamp>
            <co:creationTimestamp>2020-02-22T14:26:00.0</co:creationTimestamp>
        </co:timestamp>
        <co:eventIdentifier>
            <co:issuer href="party7"/>
            <co:identifier>trs-456123</co:identifier>
            <co:version>1</co:version>
        </co:eventIdentifier>
        <co:eventDate>2020-02-22</co:eventDate>
        <co:isCorrection>false</co:isCorrection>
        <party id="party7">
            <partyId partyIdScheme="http://www.fpml.org/coding-scheme/external/iso17442">Party 1</partyId>
        </party>
        <party id="party8">
            <partyId partyIdScheme="http://www.fpml.org/coding-scheme/external/iso17442">Party 2</partyId>
        </party>
        <!-- Lineage into the Contract formation & the 2 reset events -->
        <co:lineage>
            <co:contractReference href="c-56789"/>
            <co:eventReference href="r-1234"/>
            <co:eventReference href="r-5678"/>
        </co:lineage>
        <co:primitive>
            <!-- The transfer of the net cashflows on contract termination date -->
            <co:transfer>
                <co:settlementDate>
                    <adjustedDate>2020-02-24</adjustedDate>
                </co:settlementDate>
                <co:cashTransfer>
                    <payerPartyReference href="party7"/>
                    <receiverPartyReference href="party8"/>
                    <co:amount>
                        <currency>GBP</currency>
                        <amount>568.75</amount>
                    </co:amount>
                    <co:cashflowType>netInterest</co:cashflowType>
                    <co:breakdown>
                        <!-- Payout lineage into the swap contract and its fixed leg. -->
                        <co:lineage>
                            <co:contractReference href="c-56789"/>
                            <co:interestRatePayoutReference href="sw1234"/>
                        </co:lineage>
                        <payerPartyReference href="party7"/>
                        <receiverPartyReference href="party8"/>
                        <co:amount>
                            <currency>GBP</currency>
                            <amount>6000.00</amount>
                        </co:amount>
                        <!-- The reference to the FixedAmount vs. FloatingAmount calculation can also be used for
                    lineage purposes -->
                        <co:transferCalculation>FixedAmount</co:transferCalculation>
                    </co:breakdown>
                    <co:breakdown>
                        <!-- Payout lineage into the swap contract, its floating leg and the 2 reset events. -->
                        <co:lineage>
                            <co:contractReference href="c-56789"/>
                            <co:eventReference href="r-1234"/>
                            <co:eventReference href="r-5678"/>
                            <co:interestRatePayoutReference href="sw2345"/>
                        </co:lineage>
                        <payerPartyReference href="party8"/>
                        <receiverPartyReference href="party7"/>
                        <co:amount>
                            <currency>GBP</currency>
                            <amount>5431.25</amount>
                        </co:amount>
                        <co:transferCalculation>FloatingAmount</co:transferCalculation>
                    </co:breakdown>
                </co:cashTransfer>
                <co:status>instructed</co:status>
            </co:transfer>
        </co:primitive>
    </co:event>
</co:eventPackage>
