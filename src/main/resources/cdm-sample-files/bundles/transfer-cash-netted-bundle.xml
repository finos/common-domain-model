        <co:lineage>
            <co:contractReference href="c-56789"/>
            <co:eventReference href="o-1234"/>
        </co:lineage>
        <co:primitive>
            <co:reset>
                <co:date>2018-05-22</co:date>
                <co:resetValue>2.125</co:resetValue>
            </co:reset>
        </co:primitive>
    </co:event>
    <co:event id="o-5678">
