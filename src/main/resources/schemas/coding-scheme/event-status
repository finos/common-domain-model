   <SimpleCodeList>
      <Row>
         <Value>
            <SimpleValue>Alleged</SimpleValue>
         </Value>
         <Value>
            <SimpleValue>FpML</SimpleValue>
         </Value>
         <Value>
            <SimpleValue>No corresponding payment (or set of payments) was found in "your" submitted sets.</SimpleValue>
         </Value>
      </Row>
