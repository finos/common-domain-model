<?xml version="1.0" encoding="UTF-8"?>
<gcl:CodeList xmlns:gcl="http://xml.genericode.org/2004/ns/CodeList/0.2/" xmlns:doc="http://www.fpml.org/coding-scheme/documentation" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xml.genericode.org/2004/ns/CodeList/0.2/ CodeList.xsd">
   <Annotation>
      <Description>
         <doc:definition>Contains a code representing the type of an account, for example in a clearing or exchange model.</doc:definition>
         <doc:publicationDate>2016-06-13</doc:publicationDate>
      </Description>
   </Annotation>
   <Identification>
      <ShortName>accountTypeScheme</ShortName>
      <Version>1-1</Version>
      <CanonicalUri>http://www.fpml.org/coding-scheme/account-type</CanonicalUri>
      <CanonicalVersionUri>http://www.fpml.org/coding-scheme/account-type-1-1</CanonicalVersionUri>
      <LocationUri>http://www.fpml.org/coding-scheme/account-type-1-1.xml</LocationUri>
   </Identification>
   <ColumnSet>
      <Column Id="Code" Use="required">
         <ShortName>Code</ShortName>
         <Data Type="token">
            <Parameter ShortName="maxLength">63</Parameter>
         </Data>
      </Column>
      <Column Id="Source" Use="optional">
         <ShortName>Source</ShortName>
         <Data Type="string"/>
      </Column>
      <Column Id="Description" Use="optional">
         <ShortName>Description</ShortName>
         <Data Type="string"/>
      </Column>
      <Key Id="PrimaryKey">
         <ShortName>key</ShortName>
         <ColumnRef Ref="Code"/>
      </Key>
   </ColumnSet>
   <SimpleCodeList>
      <Row>
         <Value>
            <SimpleValue>AggregateClient</SimpleValue>
         </Value>
         <Value>
            <SimpleValue>FpML</SimpleValue>
         </Value>
         <Value>
            <SimpleValue>Aggregate client account, as defined under ESMA MiFIR.</SimpleValue>
         </Value>
      </Row>
      <Row>
         <Value>
            <SimpleValue>Client</SimpleValue>
         </Value>
         <Value>
            <SimpleValue>FpML</SimpleValue>
         </Value>
         <Value>
            <SimpleValue>The account contains trading activity or positions that belong to a client of the firm that opened the account.</SimpleValue>
         </Value>
      </Row>
      <Row>
         <Value>
            <SimpleValue>House</SimpleValue>
         </Value>
         <Value>
            <SimpleValue>FpML</SimpleValue>
         </Value>
         <Value>
            <SimpleValue>The account contains proprietary trading activity or positions, belonging to the firm that is the owner of the account.</SimpleValue>
         </Value>
      </Row>
   </SimpleCodeList>
</gcl:CodeList>