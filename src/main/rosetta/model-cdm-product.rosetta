data rule Execution_executionVenue <"When the execution type is set to 'Electronically', the execution venue must be specified.">
	when Execution -> executionType = ExecutionTypeEnum.Electronic
	then Execution -> executionVenue exists

data rule EquityPayout_singleUnderlier <"In the case of an equity payout with a single underlier, that underlier needs to be an equity product.">
	when EquityPayout -> underlier -> singleUnderlier exists
	then ( EquityPayout -> underlier -> singleUnderlier -> underlyingProduct -> security -> equity
		or EquityPayout -> underlier -> singleUnderlier -> underlyingProduct -> security -> exchangeTradedFund
		or EquityPayout -> underlier -> singleUnderlier -> underlyingProduct -> security -> mutualFund
		or EquityPayout -> underlier -> singleUnderlier -> underlyingProduct -> security -> warrant ) exists

