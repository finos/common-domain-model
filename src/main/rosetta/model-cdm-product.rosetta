		[synonym FpML_5_10, CME_SubmissionIRS_1_0, DTCC_11_0, DTCC_9_0, CME_ClearedConfirm_1_17 value convertibleBond]
	equity Equity (0..1);
		[synonym FpML_5_10 value equity]
	exchangeTradedFund ExchangeTradedFund (0..1);
		[synonym FpML_5_10 value exchangeTradedFund]
	index Index (0..1);
		[synonym FpML_5_10 value index]
	loan Loan (0..1);
		[synonym FpML_5_10, CME_SubmissionIRS_1_0, DTCC_11_0, DTCC_9_0, CME_ClearedConfirm_1_17 value loan]
	mortgageBackedSecurity MortgageBackedSecurity (0..1);
