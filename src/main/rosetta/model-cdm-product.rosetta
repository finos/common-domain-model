data rule EquityPayout_dividendReturn <"An equity price return specifies price return terms, but no dividend terms.">
	when EquityPayout -> returnType = ReturnTypeEnum.Price
	then EquityPayout -> priceReturnTerms exists
	and EquityPayout -> dividendReturnTerms is absent

data rule EquityPayout_priceReturn <"An equity dividend return specifies dividend return terms, but no price terms.">
	when EquityPayout -> returnType = ReturnTypeEnum.Dividend
	then EquityPayout -> dividendReturnTerms exists
	and EquityPayout -> priceReturnTerms is absent

data rule EquityPayout_totalReturn <"An equity total return specifies price return as well as dividend terms.">
	when EquityPayout -> returnType = ReturnTypeEnum.Total
	then EquityPayout -> priceReturnTerms exists
	and EquityPayout -> dividendReturnTerms exists

