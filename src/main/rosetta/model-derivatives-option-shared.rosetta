type OptionDenomination: <"Class which corresponds to the FpML OptionDenomination.model group.">

	optionEntitlement number (1..1) <"The number of units of underlier per option comprised in the option transaction.">
		[synonym FpML_5_10, CME_SubmissionIRS_1_0, DTCC_11_0, DTCC_9_0, CME_ClearedConfirm_1_17 value "optionEntitlement"]
	entitlementCurrency string (0..1) <"The currency in which the option is denominated. The list of valid currencies is not presently positioned as an enumeration as part of the CDM because that scope is limited to the values specified by ISDA and FpML. As a result, implementers have to make reference to the relevant standard, such as the ISO 4217 standard for currency codes.">
		[metadata scheme]
		[synonym FpML_5_10, CME_SubmissionIRS_1_0, DTCC_11_0, DTCC_9_0, CME_ClearedConfirm_1_17 value "entitlementCurrency" meta currencyScheme]
	numberOfOptions number (0..1) <"The number of options comprised in the option transaction.">
		[synonym FpML_5_10, CME_SubmissionIRS_1_0, DTCC_11_0, DTCC_9_0, CME_ClearedConfirm_1_17 value "numberOfOptions"]

	condition OptionDenomination_optionEntitlement: <" In FpML the optionEntitlement element is specified as a positive decimal.">
		if OptionDenomination -> optionEntitlement exists
		then OptionDenomination -> optionEntitlement >= 0.0

	condition OptionDenomination_numberOfOptions: <" In FpML the numberOfOptions element is specified as a positive decimal.">
		if OptionDenomination -> numberOfOptions exists
		then OptionDenomination -> numberOfOptions >= 0.0

