	businessDateRange BusinessDateRange (0..1) <"A range of contiguous business days.">
		[synonym FpML_5_10, CME_SubmissionIRS_1_0, CME_ClearedConfirm_1_17 value "businessDateRange"]

	condition DateChoice: <"condition to represent an FpML choice construct.">
