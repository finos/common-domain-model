	MMBtu <"Million British thermal units">
		[synonym FpML value "MMBTU" ],
	Bbl <"Barrel">
		[synonym FpML value "BBL"],
	Gal <"Gallon">
		[synonym FpML value "GAL"],
	t <"Ton">
		[synonym FpML value "t"],
	lbs <"Pound">
		[synonym FpML value "LB"],
	oz_tr <"Troy Ounce">
		[synonym FpML value "ozt"],
	Bcf <"Million cubic feet">
		[synonym FpML value "BCF"],
	Bu <"Bushel">
		[synonym FpML value "BSH"]
/* 
