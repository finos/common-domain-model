	Equity <"Equity">
		[synonym FpML_5_10, DTCC_11_0, DTCC_9_0 value "Equity."],
	ForeignExchange <"ForeignExchange.">
		[synonym FpML_5_10, DTCC_11_0, DTCC_9_0 value "ForeignExchange"],
